{
  "name": "danack/esprintf",
  "description": "Use laminas-escaper with sprintf like function.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Danack",
      "email": "Danack@basereality.com"
      }
  ],
  "require": {
    "laminas/laminas-escaper": "^2.9",
    "php": "^8.0 || ^8.1 || ^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20",
    "squizlabs/php_codesniffer": "^3.3.1",
    "yoast/yoastcs": "^1.0"
  },
  "autoload": {
    "files": ["lib/esprintf.php"],
    "psr-0": {
      "Esprintf\\": "lib/",
      "EsprintfTest\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "8.0"
    },
    "platform-check": false
  }
}
